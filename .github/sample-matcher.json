{
  "problemMatcher": [
    {
      "owner": "next-build",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(\\./.+)$",
          "file": 1
        },
        {
          "regexp": "^\\s*Error:\\s*$"
        },
        {
          "regexp": "^\\s+\\S*x\\S* (.+)$",
          "message": 1
        },
        {
          "regexp": "\\[(.+):(\\d+):(\\d+)\\]$",
          "line": 2,
          "column": 3
        },
        {
          "regexp": "^\\s*Caused by:\\s*$"
        },
        {
          "regexp": "^\\s+(.+)$",
          "code": 1
        }
      ]
    }
  ]
}